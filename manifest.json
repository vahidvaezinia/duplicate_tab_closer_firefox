{
  "manifest_version": 2,
  "name": "Duplicate Tab Closer",
  "version": "1.0",
  "description": "Close exact-URL duplicate tabs across Firefox windows.",
  "permissions": ["tabs", "notifications", "menus", "storage"],
  "icons": {
    "16": "icons/pro-icon.svg",
    "32": "icons/pro-icon.svg",
    "48": "icons/pro-icon.svg",
    "128": "icons/pro-icon.svg"
  },
  "browser_action": {
    "default_title": "Close duplicate tabs",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/pro-icon.svg",
      "32": "icons/pro-icon.svg",
      "48": "icons/pro-icon.svg",
      "128": "icons/pro-icon.svg"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "applications": {
    "gecko": {
      "id": "duplicate-tab-closer@example.com",
      "strict_min_version": "68.0"
    }
  }
}
